{
  "name": "nevatrip",
  "connector": "rest",
  "baseURL": "https://nevatrip.ru/api",
  "operations": [
    {
      "template": {
        "method": "GET",
        "url": "https://nevatrip.ru/api/promo",
        "query": {
          "id": "{id}",
          "code": "{code}"
        },
        "responsePath": "$"
      },
      "functions": {
        "getSale": [
          "id",
          "code"
        ]
      }
    }
  ]
}

{
  "name": "nevatrip-rest",
  "version": "0.1.2",
  "description": "nevatrip",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "docker:start": "./bin/start-dbs.sh",
    "docker:stop": "./bin/stop-dbs.sh",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "pretest:ci": "npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/test\"",
    "test:ci": "lb-mocha --allow-console-logs \"dist/test\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "migrate": "node ./dist/src/migrate",
    "prestart": "npm run build",
    "start": "HOST=0.0.0.0 PORT=8000 node .",
    "start:watch": "DEBUG=nevatrip HOST=0.0.0.0 PORT=8000 tsc-watch --onSuccess \"node .\"",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "Roman @Realetive Ganin <npm@romanganin.ru> (https://romanganin.ru)",
  "license": "CC-BY-NC-ND-4.0",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/boot": "^1.5.0",
    "@loopback/build": "^2.0.6",
    "@loopback/context": "^1.21.1",
    "@loopback/core": "^1.9.0",
    "@loopback/openapi-v3": "^1.9.1",
    "@loopback/repository": "^1.10.1",
    "@loopback/rest": "^1.16.6",
    "@loopback/rest-explorer": "^1.3.1",
    "@loopback/service-proxy": "^1.3.0",
    "@types/nodemailer": "^6.2.1",
    "@types/object-hash": "^1.3.0",
    "basic-auth-token": "^0.4.2",
    "cloudpayments": "^3.0.0",
    "date-fns": "^2.0.0-beta.3",
    "loopback-connector-kv-redis": "^3.0.1",
    "loopback-connector-mongodb": "^5.0.0",
    "loopback-connector-rest": "^3.4.2",
    "loopback-connector-soap": "^5.0.0",
    "nanoid": "^2.0.3",
    "nodemailer": "^6.3.0",
    "object-hash": "^1.3.1",
    "tsc-watch": "^2.2.1",
    "tslint": "^5.18.0"
  },
  "devDependencies": {
    "@loopback/testlab": "^1.7.2",
    "@loopback/tslint-config": "^2.1.0",
    "@types/node": "^12.6.8"
  }
}
